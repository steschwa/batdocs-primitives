{
    "name": "@batdocs/compose-styles",
    "version": "2.0.0",
    "packageManager": "yarn@3.2.4",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/steschwa/batdocs-primitives.git"
    },
    "source": "src/index.ts",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
    ],
    "sideEffects": false,
    "scripts": {
        "clean": "rm -rf dist",
        "build": "tsup src/index.ts --format esm,cjs --target esnext --dts --metafile",
        "publish": "yarn npm publish --access public --tolerate-republish"
    },
    "peerDependencies": {
        "react": "*"
    },
    "devDependencies": {
        "tsup": "^6.3.0"
    }
}

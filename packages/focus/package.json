{
    "name": "@batdocs/focus",
    "version": "1.0.0",
    "packageManager": "yarn@3.2.4",
    "source": "src/index.ts",
    "main": "dist/index.js",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "README.md"
